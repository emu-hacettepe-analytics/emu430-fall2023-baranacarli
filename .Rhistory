library(tidyverse)
library(dslabs)
data(murders)
# first option
ggplot(data=murders)
p <- murders %>%
ggplot(aes(population/10^6, total, label
= abb))
p + geom_point(size = 3) +
geom_text(nudge_x = 0.075) +
scale_x_log10() +
scale_y_log10() +
xlab("Populations in millions (log scale)") +
ylab("Total number of murders (log scale)") +
